# $OpenPave$

MOD_DEPTH	= .
topsrcdir	= @top_srcdir@
srcdir		= @srcdir@
VPATH		= @srcdir@

OP_PROJECTS	= @OP_PROJECTS@
OP_MODULES	= @OP_MODULES@

include $(MOD_DEPTH)/build/autoconf.mk

